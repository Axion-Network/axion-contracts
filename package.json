{
  "name": "axion-contracts",
  "version": "1.0.0",
  "scripts": {
    "test": "npx truffle test",
    "compile": "npx truffle compile --all",
    "build": "npx truffle compile --all && npx typechain --target=truffle-v5 'build/contracts/*.json'",
    "build:win": "npx truffle compile --all && npx typechain --target=truffle-v5 ./build/contracts/*.json",
    "build:migration": "tsc -p ./tsconfig.migrate.json --outDir ./migrations",
    "migrate": "rm -fr ./migrations && npm run build:migration && truffle migrate",
    "migrate:ropsten": "rm -fr ./migrations && npm run build:migration && truffle migrate --network ropsten",
    "migrate:live": "rm -fr ./migrations && npm run build:migration && truffle migrate --network live"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rock-n-Block/axion-contracts.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rock-n-Block/axion-contracts/issues"
  },
  "homepage": "https://github.com/Rock-n-Block/axion-contracts#readme",
  "dependencies": {
    "@openzeppelin/contracts": "^3.0.1",
    "@openzeppelin/contracts-upgradeable": "^3.3.0",
    "@openzeppelin/upgrades": "^2.8.0",
    "@truffle/hdwallet-provider": "^1.2.0",
    "@types/lodash": "^4.14.165",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "ansi-colors": "^4.1.1",
    "bn.js": "^5.1.3",
    "chai": "^4.2.0",
    "chai-bn": "^0.2.1",
    "dotenv": "^8.2.0",
    "eth-crypto": "^1.7.0",
    "ganache-core": "^2.13.1",
    "semver": "^7.3.2",
    "solidity-coverage": "^0.7.9",
    "truffle": "^5.1.56",
    "truffle-plugin-verify": "^0.5.0",
    "truffle-verify": "^1.0.8",
    "web3-eth-contract": "^1.3.0"
  },
  "devDependencies": {
    "@openzeppelin/truffle-upgrades": "^1.3.1",
    "@typechain/truffle-v5": "^3.0.0",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.4",
    "@types/web3": "^1.2.2",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "typechain": "^4.0.0",
    "typescript": "^4.1.2"
  }
}
